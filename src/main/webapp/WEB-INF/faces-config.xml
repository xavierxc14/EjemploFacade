<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <application>
        <resource-bundle>
            <base-name>/Bundle</base-name>
            <var>bundle</var>
        </resource-bundle>
    </application>
    <application>
        <el-resolver>ec.edu.epn.libres.conexion.persistencia.dao.generico.test.util.JsfCrudELResolver</el-resolver>
    </application>
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>welcome</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>estudiante</managed-bean-name>
        <managed-bean-class>ec.edu.epn.libres.ejemplofacade.persistencia.dao.generico.test.EstudianteController</managed-bean-class>
    
        <managed-bean-scope>session</managed-bean-scope></managed-bean>
    <managed-bean>
        <managed-bean-name>estudianteJpa</managed-bean-name>
        <managed-bean-class>ec.edu.epn.libres.ejemplofacade.persistencia.dao.generico.test.EstudianteFacade</managed-bean-class>
    
        <managed-bean-scope>session</managed-bean-scope></managed-bean>
    <converter>
        <converter-for-class>ec.edu.epn.libres.ejemplofacade.persistencia.entidades.Estudiante</converter-for-class>
        <converter-class>ec.edu.epn.libres.ejemplofacade.persistencia.dao.generico.test.EstudianteConverter</converter-class>
    </converter>
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>estudiante_create</from-outcome>
            <to-view-id>/pages/estudiante/New.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>estudiante_list</from-outcome>
            <to-view-id>/pages/estudiante/List.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>estudiante_edit</from-outcome>
            <to-view-id>/pages/estudiante/Edit.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>estudiante_detail</from-outcome>
            <to-view-id>/pages/estudiante/Detail.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>profesor</managed-bean-name>
        <managed-bean-class>ec.edu.epn.libres.ejemplofacade.persistencia.dao.generico.test.ProfesorController</managed-bean-class>
    
        <managed-bean-scope>session</managed-bean-scope></managed-bean>
    <managed-bean>
        <managed-bean-name>profesorJpa</managed-bean-name>
        <managed-bean-class>ec.edu.epn.libres.ejemplofacade.persistencia.dao.generico.test.ProfesorFacade</managed-bean-class>
    
        <managed-bean-scope>session</managed-bean-scope></managed-bean>
    <converter>
        <converter-for-class>ec.edu.epn.libres.ejemplofacade.persistencia.entidades.Profesor</converter-for-class>
        <converter-class>ec.edu.epn.libres.ejemplofacade.persistencia.dao.generico.test.ProfesorConverter</converter-class>
    </converter>
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>profesor_create</from-outcome>
            <to-view-id>/pages/profesor/New.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>profesor_list</from-outcome>
            <to-view-id>/pages/profesor/List.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>profesor_edit</from-outcome>
            <to-view-id>/pages/profesor/Edit.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>profesor_detail</from-outcome>
            <to-view-id>/pages/profesor/Detail.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
